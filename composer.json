{
    "name": "kyy1996/think-migration",
    "description": "A package enables thinkphp5 to use Phinx to manage the database migration.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "robmorgan/phinx": "^0.9.2",
        "topthink/framework": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5.7|^6.5"
    },
    "license": "MIT",
    "keywords": ["thinkphp", "phinx", "migrations", "database", "db", "database migrations"],
    "homepage": "https://www.kyy1996.com",
    "version": "1.0",
    "authors": [
        {
            "name": "kyy1996",
            "email": "system@kyy1996.com"
        }
    ],
    "autoload": {
        "psr-4": {
           "think\\migration\\": "src"
        },
        "files": ["bootstrap.php"]
    }
}
